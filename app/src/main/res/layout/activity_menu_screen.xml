<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuScreen"
    android:background="@drawable/menuscreen">

    <ImageView
        android:id="@+id/about_app_button_id"
        android:layout_width="306dp"
        android:layout_height="70dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="16dp"
        android:background="@drawable/rec"
        app:layout_constraintBottom_toTopOf="@+id/avail_material_button_id"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingStart="10dp"
        android:src="@drawable/ic_rect"
        app:layout_constraintBottom_toBottomOf="@+id/about_app_button_id"
        app:layout_constraintStart_toStartOf="@+id/about_app_button_id"
        app:layout_constraintTop_toTopOf="@+id/about_app_button_id" />


    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:src="@drawable/square"
        app:layout_constraintBottom_toBottomOf="@+id/imageView9"
        app:layout_constraintEnd_toEndOf="@+id/imageView9"
        app:layout_constraintStart_toStartOf="@+id/about_app_button_id"
        app:layout_constraintTop_toTopOf="@+id/about_app_button_id"
        app:layout_constraintVertical_bias="0.666" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:text="i"
        android:textColor="@color/white"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="@+id/about_app_button_id"
        app:layout_constraintEnd_toEndOf="@+id/about_app_button_id"
        app:layout_constraintHorizontal_bias="0.109"
        app:layout_constraintStart_toStartOf="@+id/about_app_button_id"
        app:layout_constraintTop_toTopOf="@+id/about_app_button_id"
        app:layout_constraintVertical_bias="0.488" />

    <ImageView
        android:id="@+id/login_button_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="64dp"
        android:background="@drawable/rec"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:src="@drawable/ic_rect"
        app:layout_constraintBottom_toBottomOf="@+id/login_button_id"
        app:layout_constraintStart_toStartOf="@+id/login_button_id"
        app:layout_constraintTop_toTopOf="@+id/login_button_id" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/arrow"
        app:layout_constraintBottom_toBottomOf="@+id/login_button_id"
        app:layout_constraintEnd_toEndOf="@+id/login_button_id"
        app:layout_constraintHorizontal_bias="0.082"
        app:layout_constraintStart_toStartOf="@+id/login_button_id"
        app:layout_constraintTop_toTopOf="@+id/login_button_id"
        app:layout_constraintVertical_bias="0.533" />

    <ImageView
        android:id="@+id/avail_material_button_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="16dp"
        android:background="@drawable/rec"
        app:layout_constraintBottom_toTopOf="@+id/login_button_id"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:src="@drawable/ic_rect"
        app:layout_constraintBottom_toBottomOf="@+id/avail_material_button_id"
        app:layout_constraintStart_toStartOf="@+id/avail_material_button_id"
        app:layout_constraintTop_toTopOf="@+id/avail_material_button_id" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/clock"
        app:layout_constraintBottom_toBottomOf="@+id/imageView8"
        app:layout_constraintEnd_toEndOf="@+id/imageView8"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toTopOf="@+id/imageView8" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ABOUT THE APP"
        android:textColor="#166FFF"
        android:textSize="20sp"
        android:textStyle="bold"
        android:layout_marginStart="100dp"
        app:layout_constraintBottom_toBottomOf="@+id/about_app_button_id"
        app:layout_constraintStart_toStartOf="@+id/about_app_button_id"
        app:layout_constraintTop_toTopOf="@+id/about_app_button_id" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="AVAILABLE MATERIAL"
        android:layout_marginStart="100dp"
        android:textColor="#166FFF"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/avail_material_button_id"
        app:layout_constraintStart_toStartOf="@+id/avail_material_button_id"
        app:layout_constraintTop_toTopOf="@+id/avail_material_button_id" />


    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:text="LOG IN"
        android:textStyle="bold"
        android:textSize="18dp"
        android:textColor="#166FFF"
        app:layout_constraintBottom_toBottomOf="@+id/login_button_id"
        app:layout_constraintStart_toStartOf="@+id/login_button_id"
        app:layout_constraintTop_toTopOf="@+id/login_button_id" />

</androidx.constraintlayout.widget.ConstraintLayout>